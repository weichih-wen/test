<%= form_for @photo do |f| %>
  <%= f.label :photo_name, "Title" %>
  <%= f.text_field :photo_name %>

  <%= f.label :date, "Date" %>
  <%= f.text_field :date %>

  <%= f.label :description, "Description" %>
  <%= f.text_area :description %>

  <%= f.label :file_location, "File Location" %>
  <%= f.text_field :file_location %>

  <%= f.label :tag, "Tag" %>
  <%= f.text_field :tag %>

  <%= f.submit "Update" %>
<% end %>